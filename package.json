  "contributes": {
    "languages": [
      {
        "id": "sql",
        "extensions": [
          ".sql"
        ],
        "aliases": [
          "SQL"
        ]
      }
    ],
    "outputChannels": [
      "PGSQL"
    ],
    "snippets": [
      {
        "language": "sql",
        "path": "./snippets/pgsql.json"
      }
